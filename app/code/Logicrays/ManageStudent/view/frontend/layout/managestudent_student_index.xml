<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd"> 
    <update handle="customer_account"/>
    <head>
        <css src="Logicrays_ManageStudent::css/styles.css" />
    </head>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_page_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Logicrays\ManageStudent\Block\Students" name="managestudent_students" template="Logicrays_ManageStudent::managestudent/manage_student.phtml" cacheable="false"/>
            <block class="Logicrays\ManageStudent\Block\Students\Grid" name="managestudent_students_grid" template="Logicrays_ManageStudent::managestudent/student/grid.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>